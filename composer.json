{
    "name": "omnicode/lara-repo",
    "description": "Repository pattern for Laravel",
    "keywords": ["laravel", "repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "OmniCode"
        }
    ],
    "require": {
        "laravel/framework": "~5.0",
        "omnicode/lara-tools": "~0.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "LaraRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}

