{
    "name": "omnicode/lara-repo",
    "description": "Repository pattern for Laravel",
    "keywords": ["laravel", "repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "OmniCode"
        }
    ],
    "require": {
        "laravel/framework": "5.5.*",
        "omnicode/lara-support": "~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0",
        "omnicode/lara-test": "~2.0"
    },
    "suggest": {
        "omnicode/lara-crud": "LaraRepo integrated with LaraCrud",
        "omnicode/lara-service": "LaraRepo integrated with LaraService",
        "omnicode/lara-model": "LaraRepo integrated with LaraModel"
    },
    "autoload": {
        "psr-4": {
            "LaraRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}

