{
    "name": "omnicode/lara-repo",
    "description": "Repository pattern for Laravel",
    "keywords": ["laravel", "repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "OmniCode"
        }
    ],
    "require": {
        "laravel/framework": "5.5.*",
        "omnicode/lara-support": "~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "LaraRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}

